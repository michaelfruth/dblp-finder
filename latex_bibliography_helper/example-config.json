        "hide": true
      },
      "pages",
      {
        "name": "publisher",
        "hide": true
      },
      "year",
      {
        "name": "month",
        "hide": true
      },
      "isbn",
      {
        "name": "url",
        "hide": true
      },
      {
        "name": "doi",
        "hide": true
